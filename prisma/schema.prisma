generator client {
  provider = "prisma-client-js"
  output   = "../generated/prisma"
}

datasource db {
  provider = "postgresql"
}
enum Role {
  FRONTEND_DEVELOPER
  BACKEND_DEVELOPER
  FULLSTACK_DEVELOPER
  UI_UX_DESIGNER
  DEVOPS_ENGINEER
  QA_ENGINEER
  SECURITY_ENGINEER
  DATA_SCIENTIST
  AI_ML_ENGINEER
  PRODUCT_MANAGER
  PROJECT_MANAGER
  TECHNICAL_WRITER
  CONTENT_CREATOR
  COMMUNITY_MANAGER
  STUDENT
  EDUCATOR
  FOUNDER
  IT_SUPPORT
  BUSINESS_ANALYST
  SMART_CONTRACT_DEVELOPER
  BLOCKCHAIN_DEVELOPER
  WEB3_DEVELOPER
  SOLIDITY_DEVELOPER
  DAPP_DEVELOPER
  TOKENOMICS_SPECIALIST
  NFT_DEVELOPER
  DEFI_DEVELOPER
  WEB3_SECURITY_AUDITOR
  BLOCKCHAIN_ARCHITECT
  OTHER
}

model EventRegistration {
  id                    String   @id @default(cuid())
  firstName             String
  lastName              String
  email                 String   @unique
  isCommunityMember     Boolean
  role                  Role
  roleOther             String?  // if role is OTHER
  location              String
  locationOther         String?  // if location is others
  openSourceKnowledge   Int      @db.SmallInt // 1-10 scale
  createdAt             DateTime @default(now())
  
  @@map("event_registrations")
}


